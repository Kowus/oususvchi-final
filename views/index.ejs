<!-- Header area -->
<header id="header">
    <div class="center text-center">
        <h1 class="bigheadline">Ousu Svchi</h1>
        <h4 class="subheadline">Life In A Pixel</h4>
        <p id="kowus" style="color: black; font-size: 2em;"></p>
    </div>
    <div class="bottom">
        <a data-scroll href="#navigation" class="scrollDown animated pulse" id="scrollToContent"><i
                    class="pe-7s-angle-down-circle pe-va"></i></a>
    </div>
</header>

<!-- Navigation area -->
<section id="navigation" ng-controller="NavController as navctrl">
    <div class="container">
        <div class="row">
            <div class="col-xs-6">
                <!--<div class="logo"><a data-scroll href="#body" class="logo-text">Ousu Svchi</a></div>-->
                <div class="logo logo-text">Ousu Svchi</div>
            </div>
            <div class="col-xs-6">
                <div class="nav">
                    <a href="#" data-placement="bottom" title="Menu" class="menu" data-toggle="dropdown"
                       target="_self"><i
                                class="pe-7s-menu"></i></a>
                    <div class="dropdown-menu">
                        <div class="arrow-up"></div>
                        <ul>
                            <!-- angular array for nav-->
                            <li ng-repeat="nav in navctrl.navs"><a data-scroll href="{{nav.href}}" target="_self">{{nav
                                    .name}}<i
                                            class="{{nav.class}}"></i></a><span class="menu-effect"></span></li>
                        </ul>
                    </div>
                </div>
            </div>

        </div>
    </div>
</section>

<!-- Content Area -->

<!-- services section -->

<section id="services" class="service-area" ng-controller="ServicesController as servi">
    <div class="container">
        <h2 class="block_title">Services</h2>
        <div class="row"><a name="services"></a>
            <div class="col-md-3 col-sm-6" ng-repeat="serv in servi.service">
                <div class="services">
                    <div class="service-wrap">
                        <i class="{{serv.icon}}"></i>
                        <a href="{{serv.link}}" target="_self" class="ouslinks">
                            <h3>{{serv.head}}</h3>
                            <p>{{serv.content}}</p>
                        </a>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>
<!-- services -->

<!-- Portfolio Area -->

<section id="portfolio" class="portfolio-area" ng-controller="PortfolioController as port">
    <div class="container">
        <h2 class="block_title">Portfolio</h2><a name="portfolio"></a>
        <div class="row port cs-style-3">
            <div class="col-md-5 col-sm-6 col-xs-12 item-space" ng-repeat="work in port.works" ng-show="{{work.show}}">
                <a name="{{work.link}}"></a>
                <figure>
                    <img ng-src="{{work.image}}" alt="{{work.name}}">
                    <figcaption>
                        <h3>{{work.name}}</h3>
                        <span>{{work.description}}</span>

                        <!--a href="{{work.link}}" target="_blank" class="button" >Take a look</a-->
                        <a href="{{work.id2}}" class="button htem2" data-toggle="collapse">Take a look</a>

                    </figcaption>

                    <a href="{{work.id2}}" class="button htem col-sm-3" data-toggle="collapse"
                       style="color: steelblue; background-color: #333333; padding: 10px 8px">Take a
                        look</a>
                </figure>

                <p></p>
                <div class="collapse" id="{{work.id}}">
                    <div ng-include='work.ext'></div>


                </div>

                <div class="col-xs-12">
                    <div class="btn-center">
                        <a href="#" class="big button" style="border: 0px;"> </a>
                    </div>
                </div>
            </div>

        </div>
    </div>
    <!-- container -->

</section>
<!-- portfolio -->

<!-- Testimonial Area -->

<section id="testimonial" class="testimonial-area" ng-controller="TestimonialController as tescon">
    <div class="container"><a name="testimonials"></a>
        <h2 class="block_title">Testimonials</h2>
        <div class="row">
            <!--div class="col-xs-12">
            </div-->
            <div id="testimonial-container" class="col-xs-12">
                <div class="testimonila-block" ng-repeat="item in tescon.test">
                    <img ng-src="{{item.pic}}" alt="clients" class="selfshot">
                    <p>{{item.content}}</p>
                    <strong>{{item.name}}</strong>
                    <br>
                    <small>{{item.title}}</small>
                </div>
            </div>
        </div>
    </div>
    <!-- container -->
</section>
<!-- testimonial -->

<!-- Contact Area -->

<section id="contact" class="mapWrap" ng-controller="ProfileController as proctrl">
    <div id="googleMap" style="width:100%;"></div>
    <div id="contact-area">
        <div class="container">
            <h2 class="block_title">Hey !!!</h2>
            <div class="row">
                <div class="col-xs-12">
                </div>
                <div class="col-sm-6">
                    <div class="moreDetails">
                        <h2 class="con-title">Profile</h2>
                        <p><%= about %><br></p>

                        <br><span style="color: violet; font-size: 1.2em; font-weight: bold;">Background</span><br>
                        <div ng-repeat="pro in proctrl.prof.background">
                            <p>
                                <br><span style="font-style: italic; font-weight: bold;">{{pro.head}}</span><br>
                                <span style="font-style: italic; margin-left: 10px; font-size: 0.8em;">{{pro.cont}}</span><br>
                            </p>
                        </div>
                        <ul class="address">
                            <!--<li><i class="pe-7s-map-marker"></i><span>1600 Pennsylvania Ave NW,<br>Washington, DC 20500,<br>United States</span></li>-->
                            <li><i class="pe-7s-mail"></i><span>oususvchi@gmail.com</span></li>
                            <li><i class="pe-7s-phone"></i><span>+233-20-901-3742</span></li>
                            <li><i class="pe-7s-global"></i><span><a href="#body">www.oususvchi.com</a></span></li>
                        </ul>
                    </div>
                </div>
                <div class="col-sm-6">
                    <h2 class="con-title">Let your voice be heard</h2>
                    <form role="form" action="myapi" method="POST">
                        <div class="form-group">
                            <input type="text" name="name" class="form-control" required placeholder="Enter your name"
                                   value="<%= name %>">
                        </div>
                        <div class="form-group">
                            <input type="email" id="mail" name="mail" class="form-control" required
                                   placeholder="Enter your email" value="<%= email %>">

                        </div>
                        <div class="form-group">
                            <p id="kowus"></p>
                            <textarea name="message" class="form-control" rows="5" required
                                      placeholder="Let this space be your scratchpad, let your voice be heard, share opinions, suggestions, how can I be better, want to book me for an event? I do appreciate your taking the time to visit my page. Your feedback really means a lot to me! "><%= message %></textarea>
                        </div>

                        <button type="submit" class="btn medium">Hit me up</button>
                        <a name="contact"></a>
                    </form>
                    <p id="statusMessage" class="alert alert-success" style="width:96%; text-align: center;">
                        <span id="messCon"><%= stat %></span>
                        <a href="#" name="bot" class="close" data-dismiss="alert" aria-label="close">&times;</a>
                    </p></div>
                <p id="some"><%= mstus %></p>
            </div>
        </div>
    </div>
    <!-- container -->


    <!-- contact-area -->
    <div id="social">
        <div class="container">
            <div class="row">
                <div class="col-xs-12">
                    <ul class="scoialinks">
                        <li class="normal-txt">Find me on</li>
                        <li class="social-icons">
                            <a class="facebook" href="#"></a>
                        </li>
                        <li class="social-icons">
                            <a class="twitter" href="#"></a>
                        </li>
                        <li class="social-icons">
                            <a class="linkedin" href="#"></a>
                        </li>
                        <li class="social-icons">
                            <a class="google-plus" href="#"></a>
                        </li>
                        <li class="social-icons">
                            <a class="wordpress" href="#"></a>
                        </li>
                    </ul>
                </div>
            </div>
        </div>
    </div>

    <!-- social -->
</section>
